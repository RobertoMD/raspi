<html><head>
<script>
function load() {
	var pinx = document.getElementById("pinx");
	var f = document.getElementById("pinselect");
	f.btngo.disabled=true;
	f.pin.value="";
	pinx.innerHTML="";
}
function input(e) {
	var pinx = document.getElementById("pinx");
	var f = document.getElementById("pinselect");
	if (pinx.innerHTML.length<4){
		pinx.innerHTML = pinx.innerHTML + "*";
		var pin = document.getElementById("pin");
		pin.value = pin.value + e;
	}
	go();
}
function del() {
	var pin = document.getElementById('pinx');
	pinx.innerHTML = pin.innerHTML.substr(0, pin.innerHTML.length - 1);
	var pin = document.getElementById('pin');
	pin.value = pin.value.substr(0, pin.value.length - 1);
	go();
}
function go() {
	var f = document.getElementById("pinselect");
	if (document.getElementById('pinx').innerHTML.length==4) {
		f.btngo.disabled=false;
	} else {
		f.btngo.disabled=true;
	}
}
</script>
</head>
<body onload='load();'>
<div id="pinx" height="50"> </div>
<div id="tmp"> </div>
<div id="VirtualKey">
<form id="pinselect" method="post" action="/cgi-bin/main.py">
		<input id="pin" type="hidden" value="" />
		<input id="btn1" type="button" value="1" onclick="input('1');" />
		<input id="btn2" type="button" value="2" onclick="input('2');" />
		<input id="btn3" type="button" value="3" onclick="input('3');" />
		<br />
		<input id="btn4" type="button" value="4" onclick="input('4');" />
		<input id="btn5" type="button" value="5" onclick="input('5');" />
		<input id="btn6" type="button" value="6" onclick="input('6');" />
		<br />
		<input id="btn7" type="button" value="7" onclick="input('7');" />
		<input id="btn8" type="button" value="8" onclick="input('8');" />
		<input id="btn9" type="button" value="9" onclick="input('9');" />
		<br />
		<input id="btn0" type="button" value="0" onclick="input('0');" />
		<input id="btnDel" type="button" value="BS" onclick="del();" />
		<input id="btngo" type="button" value="go" onclick="document.forms[0].submit();" />
</form>
<table width=100% height=100% border=1>
<tr>
<td onclick="input('1');">1</td>
<td onclick="input('2');">2</td>
<td onclick="input('3');">3</td>
</tr>
<tr>
<td onclick="input('4');">4</td>
<td onclick="input('5');">5</td>
<td onclick="input('6');">6</td>
</tr>
<tr>
<td onclick="input('7');">7</td>
<td onclick="input('8');">8</td>
<td onclick="input('9');">9</td>
</tr>
<tr>
<td onclick="input('0');">0</td>
<td onclick="del();">bs</td>
<td onclick="document.forms[0].submit();">go</td>
</tr>
</table>
</div>
</body>
</html>
